  "BeheerdeBedrijvenzone.type": "https://data.vlaanderen.be/id/concept/RuimtelijkeEenheidtype/beheerdeBedrijvenzone",
  "BeheerdeBedrijvenzone.voorzieneMiddelen": {
    "template_type": "base_uri",
